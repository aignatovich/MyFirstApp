@model App.Models.ProjectViewModel
@using App.Models

@{
    ViewBag.Title = "Add Project";
}


<h2>Add Project</h2>
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/bootstrap-datepicker.min.js"></script>
<link rel="stylesheet" href="~/Content/bootstrap-datepicker3.min.css" />

@using (Html.BeginForm("CreateProject", "Project", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    <h4>Add new project.</h4>
    @Html.Partial("ProjectPartialForm")
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jquery")
}
<script>
    (function(){
        $('.datepicker').datepicker()
    })();
</script>




